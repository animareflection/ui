{
  "name": "@animareflection/ui",
  "description": "Anima Reflection design system and library",
  "version": "0.4.0",
  "license": "MIT",
  "packageManager": "yarn@3.6.3",
  "main": "build/ui.js",
  "module": "build/ui.mjs",
  "types": "build/ui.d.ts",
  "files": [
    "build"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/animareflection/ui"
  },
  "exports": {
    ".": {
      "require": "./build/ui.js",
      "import": "./build/ui.mjs",
      "types": "./build/ui.d.ts"
    },
    "./client": {
      "require": "./build/client.js",
      "import": "./build/client.mjs",
      "types": "./build/client.d.ts"
    },
    "./next": {
      "require": "./build/next.js",
      "import": "./build/next.mjs",
      "types": "./build/next.d.ts"
    },
    "./primitives": {
      "require": "./build/primitives.js",
      "import": "./build/primitives.mjs",
      "types": "./build/primitives.d.ts"
    },
    "./index.css": "./build/ui.css"
  },
  "scripts": {
    "dev": "panda codegen && concurrently --kill-others -n \"BUILD,PANDA,STORYBOOK\" -c \"#f13334,#ffc83d,#ff4785\" \"yarn build --watch\" \"yarn dev:css\" \"yarn storybook\"",
    "dev:css": "panda --watch",
    "prepare:css": "panda codegen",
    "build": "panda codegen && tsup",
    "format": "prettier . --write",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx src",
    "test": "test-storybook",
    "test:coverage": "yarn test --coverage",
    "test:ci": "concurrently --kill-others -s first -n \"STORYBOOK,TEST\" -c \"#ff4785,#45ba4b\" \"http-server storybook-static --port 6006 --silent\" \"wait-on tcp:6006 && yarn test:coverage --ci\"",
    "test:watch": "yarn test --watch",
    "storybook": "storybook dev --quiet --no-open -p 6006",
    "storybook:build": "panda codegen && storybook build",
    "changeset": "changeset",
    "release": "yarn build && changeset publish",
    "postinstall": "husky install",
    "prepack": "pinst --disable",
    "postpack": "pinst --enable"
  },
  "resolutions": {
    "@zag-js/store": "^0.17.0"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "yalcSig": "88f7e26945671d0ad754393b5241ede1"
}
