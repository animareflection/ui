{
  "name": "@animareflection/ui",
  "description": "Anima Reflection design system and library",
  "version": "0.3.0",
  "license": "MIT",
  "packageManager": "yarn@3.6.1",
  "main": "build/ui.js",
  "module": "build/ui.mjs",
  "types": "build/ui.d.ts",
  "files": [
    "build"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/animareflection/ui"
  },
  "exports": {
    ".": {
      "require": "./build/ui.js",
      "import": "./build/ui.mjs",
      "types": "./build/ui.d.ts"
    },
    "./client": {
      "require": "./build/client.js",
      "import": "./build/client.mjs",
      "types": "./build/client.d.ts"
    },
    "./next": {
      "require": "./build/next.js",
      "import": "./build/next.mjs",
      "types": "./build/next.d.ts"
    },
    "./primitives": {
      "require": "./build/primitives.js",
      "import": "./build/primitives.mjs",
      "types": "./build/primitives.d.ts"
    },
    "./index.css": "./build/ui.css"
  },
  "scripts": {
    "prepare:css": "panda codegen",
    "dev": "panda codegen && concurrently --kill-others -n \"BUILD,PANDA,STORYBOOK\" -c \"#f13334,#ffc83d,#ff4785\" \"yarn build --watch\" \"yarn dev:css\" \"yarn storybook\"",
    "dev:css": "panda --watch",
    "build": "panda codegen && tsup --onSuccess 'while [ ! -f ./build/ui.d.ts ] && [ ! -f ./build/client.d.ts ] && [ ! -f ./build/primitives.d.ts ] && [ ! -f ./build/next.d.ts ]; do sleep 1; done && sleep 4; yarn yalc push'",
    "format": "prettier . --write",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx src",
    "test": "panda codegen && jest",
    "test:coverage": "yarn test --coverage",
    "test:ci": "yarn test --ci",
    "test:watch": "yarn test --watch",
    "storybook": "storybook dev --quiet --no-open -p 6006",
    "storybook:build": "panda codegen && storybook build",
    "changeset": "changeset",
    "release": "yarn build && changeset publish",
    "prepare": "husky install"
  },
  "resolutions": {
    "@zag-js/store": "^0.13.0"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@ark-ui/react": "^0.13.1",
    "@changesets/changelog-github": "^0.4.8",
    "@changesets/cli": "^2.26.2",
    "@jest/globals": "^29.6.2",
    "@pandacss/dev": "^0.11.1",
    "@storybook/addon-a11y": "^7.3.2",
    "@storybook/addon-essentials": "^7.3.2",
    "@storybook/addon-interactions": "^7.3.2",
    "@storybook/addon-links": "^7.3.2",
    "@storybook/blocks": "^7.3.2",
    "@storybook/builder-webpack5": "^7.3.2",
    "@storybook/manager-api": "^7.3.2",
    "@storybook/nextjs": "^7.3.2",
    "@storybook/react": "^7.3.2",
    "@storybook/storybook-deployer": "^2.8.16",
    "@storybook/testing-library": "^0.2.0",
    "@storybook/theming": "^7.3.2",
    "@swc/core": "^1.3.74",
    "@swc/jest": "^0.2.27",
    "@testing-library/dom": "^9.3.1",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/node": "^20.4.7",
    "@types/react": "^18.2.18",
    "@types/testing-library__jest-dom": "^5.14.9",
    "@typescript-eslint/eslint-plugin": "^6.2.1",
    "@typescript-eslint/parser": "^6.2.1",
    "concurrently": "^8.2.0",
    "eslint": "^8.46.0",
    "eslint-config-prettier": "^8.10.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-import": "^2.28.0",
    "eslint-plugin-jest": "^27.2.3",
    "eslint-plugin-jest-dom": "^5.0.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-react": "^7.33.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-storybook": "^0.6.13",
    "eslint-plugin-testing-library": "^5.11.0",
    "eslint-plugin-typescript-sort-keys": "^2.3.0",
    "eslint-plugin-unused-imports": "^3.0.0",
    "husky": "^8.0.3",
    "jest": "^29.6.2",
    "jest-environment-jsdom": "^29.6.2",
    "lint-staged": "^13.2.3",
    "next": "^13.4.19",
    "postcss": "^8.4.27",
    "prettier": "^3.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.10.1",
    "storybook": "^7.3.2",
    "storybook-dark-mode": "^3.0.1",
    "ts-node": "^10.9.1",
    "tsup": "^7.2.0",
    "typescript": "^5.1.6",
    "webpack": "^5.88.2",
    "yalc": "^1.0.0-pre.53"
  }
}
